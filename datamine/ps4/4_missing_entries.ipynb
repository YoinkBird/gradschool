    "def printranks(df, wantrank=0, savefig=False):\n",
    "    # https://docs.scipy.org/doc/numpy/reference/generated/numpy.linalg.svd.html\n",
    "    # create U,s,V and then re-create the original matrix for verification\n",
    "    # Partial SVD\n",
    "    U,s_arr,V = print_partial_svd_reconstruction(df)\n",
    "\n",
    "    # Full SVD\n",
    "    U,s_arr,V = print_full_svd_reconstruction(df)\n",
    "\n",
    "    # only look at the low ranks - keep values close to zero\n",
    "    # isclose : all values close to '0'\n",
    "    # invert(isclose) : all values not close to '0'\n",
    "    # s2_arr[invert(isclose)] = 0 : zero-out values not close to '0' . could also use operator '~'\n",
    "    s2_arr = np.copy(s_arr)\n",
    "    # not sure: s2_arr[np.invert(np.isclose(s2_arr,0))] = 0\n",
    "    s2_arr[np.isclose(s_arr,0)] = 0\n",
    "    # plot the input data\n",
    "    fig = plt.figure()\n",
    "    ax1 = fig.add_subplot(111)\n",
    "    ax1.scatter(df[0],df[1])\n",
    "    # use s_arr, not s2_arr, in case s2_arr gets shortened by removing zeros\n",
    "    ax1.set_title(\"original (rank = %d)\" % len(s_arr))\n",
    "    plt.show()\n",
    "    #\n",
    "\n",
    "    print(\"-I-: reconstructions WITHOUT ranks close to zero\")\n",
    "    #  Show various rank reconstructions\n",
    "    # strategy: show each of the first 'showall', then in every 'interval'\n",
    "    # use this to narrow down the ranks\n",
    "    # length of array, including near-zero ranks\n",
    "    arrlen = len(s2_arr)\n",
    "    # show all non-near-zero ranks:\n",
    "    showall = len(s2_arr[~np.isclose(s2_arr,0)])\n",
    "    # show additional reconstructions for every n'th rank after 'showall'th rank\n",
    "    interval = int(arrlen/showall)\n",
    "    if(interval <= showall):\n",
    "        interval = showall+1\n",
    "    # kvals = len(s2_arr) + all non-near-zero ranks + a few near-zero ranks\n",
    "    kvals = [len(s2_arr)]\n",
    "    # each value up to and including interval\n",
    "    ones = range(1,showall+1)\n",
    "    kvals.extend(ones)\n",
    "    # every interval'th value\n",
    "    kvals.extend(range(interval,arrlen,interval))\n",
    "    # max value again for reference at end\n",
    "    kvals.append(len(s2_arr))\n",
    "    print(\"array: %d ; nonzeros: %d ; showall: %d ; interval: %d ;\" % (len(s2_arr),arrlen,showall,interval))\n",
    "    print(kvals)\n",
    "    if(wantrank != 0):\n",
    "      kvals = [wantrank]\n",
    "    for ind,kval in enumerate(kvals):\n",
    "      S2 = expand_s_to_S(df, s_arr[:kval])\n",
    "      arrk = svd_reconstruct(U,S2,V)\n",
    "      # recreate (or create new) matrix\n",
    "      df_reconst = pd.DataFrame(arrk)\n",
    "      if(wantrank != 0):\n",
    "        return df_reconst\n",
    "    #  ax1.scatter(df_reconst[0],df_reconst[1],c='r')\n",
    "      print(\"rank: %d\" % kval)\n",
    "      tmptitle = \"rank: %d\" % kval\n",
    "      if(kval == len(s_arr)):\n",
    "        tmptitle = \"max \" + tmptitle\n",
    "      df_reconst.plot.scatter(x=0,y=1,title=tmptitle)\n",
    "      print(\"new shape:\")\n",
    "      print(df_reconst.shape)\n",
    "      if(savefig):\n",
    "        img_out_relpath = \"svd_%s%d_r%03d.png\" % (file_subject,detail,kval)\n",
    "        img_out_relpath = \"out4/\" + img_out_relpath\n",
    "        plt.savefig(img_out_relpath)\n",
    "      plt.show()\n",
    "      # are they all close to 0?\n",
    "    #  print(\"=~0?\" + np.isclose((df - df_reconst),0).all())\n",
    "      toler = 0.5\n",
    "    #  print(\"=~0,+-\"+toler+\"?\" + np.isclose((df - df_reconst),0,atol=toler).all())\n",
    "      print(\"\")"
   ]
